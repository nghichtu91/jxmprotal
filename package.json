{
  "name": "react-antd-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/plots": "^1.2.4",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.5",
    "@reduxjs/toolkit": "^1.9.1",
    "antd": "^5.1.5",
    "axios": "^1.2.3",
    "dayjs": "^1.11.7",
    "driver.js": "^0.9.8",
    "history": "^5.3.0",
