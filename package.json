    "react-css-theme-switcher": "^0.3.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.5",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.2",
    "recharts": "^2.3.2"
  },
  "scripts": {
    "postinstall": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview --port 8000 ",
    "lint": "eslint . --ext js,ts,tsx",
    "format": "prettier --write **/*.{js,ts,tsx} && eslint . --ext js,ts,tsx --fix",
    "---------------GIT-------------------": "------",
    "git:getReleasesAndTags": "git tag --points-at HEAD | tr '\\r\\n' ' '",
    "git:getCommitSHA": "git rev-parse HEAD",
    "git:getCommitSHA:short": "git rev-parse --short HEAD",
    "git:getCommitRef": "git symbolic-ref HEAD"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@emotion/babel-plugin": "^11.10.5",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.25",
    "@types/recharts": "^1.8.24",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-plugin-import": "^1.13.6",
    "classnames": "^2.3.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.0",
    "moment": "^2.29.4",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.1",
    "terser": "^5.16.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-imp": "^2.3.1",
    "vite-plugin-svgr": "^2.4.0",
    "yarn-upgrade-all": "^0.7.2"
